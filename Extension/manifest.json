{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "web_accessible_resources": [
        {
            "resources" : ["data/cache.json", "urldata.csv"],
            "matches" : ["<all_urls>"]
        }
    ]
}